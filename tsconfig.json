{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/Calender/index.jsx", "components/Checkboxes/CheckboxOne.jsx", "components/Dashboard/E-commerce.jsx", "app/admin/components/auth/signin/page.jsx", "app/admin/components/auth/signup/page.jsx", "app/admin/components/Sidebar/SidebarLinkGroup.jsx"],
  "exclude": ["node_modules"]
}
